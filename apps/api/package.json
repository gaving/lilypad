{
  "name": "@lilypad/api",
  "version": "10.0.0",
  "description": "Lilypad API - Express server for Docker container management",
  "type": "module",
  "main": "build/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch server.ts",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "format": "biome format . --write",
    "start": "node build/server.js",
    "clean": "rm -rf node_modules build"
  },
  "dependencies": {
    "@types/got": "^9.6.12",
    "dotenv": "^17.3.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "got": "^14.6.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@lilypad/config": "workspace:*",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.3",
    "nodemon": "^3.1.10",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
