{
  "name": "lilypad",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build && cp -r build ../server",
    "eject": "react-scripts eject",
    "lint": "DEBUG=eslint:cli-engine eslint --ignore-pattern 'build/*' --ignore-pattern 'public/*' --ext js --ext json --color .",
    "start": "concurrently 'react-scripts start' 'npm start --prefix ../server'",
    "test": "react-scripts test"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@blueprintjs/core": "^4.12.0",
    "@blueprintjs/select": "^4.8.11",
    "framer-motion": "^7.6.7",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-emoji-render": "^1.2.4",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "react-scripts": "^5.0.1",
    "rebass": "^4.0.7",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "concurrently": "^7.6.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json-files": "^1.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unicorn": "^45.0.0",
    "http-proxy-middleware": "^2.0.6",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1"
  }
}
