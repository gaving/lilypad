{
  "name": "lilypad",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build && cp -r build ../server",
    "eject": "react-scripts eject",
    "lint": "DEBUG=eslint:cli-engine eslint --ignore-pattern 'build/*' --ignore-pattern 'public/*' --ext js --ext json --color .",
    "start": "concurrently 'react-scripts start' 'npm start --prefix ../server'",
    "test": "react-scripts test"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@blueprintjs/core": "^5.4.0",
    "@blueprintjs/select": "^5.0.14",
    "framer-motion": "^10.16.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-emoji-render": "^2.0.1",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.16.0",
    "react-scripts": "^5.0.1",
    "rebass": "^4.0.7",
    "styled-components": "^6.0.8"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "concurrently": "^8.2.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-json-files": "^3.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "http-proxy-middleware": "^2.0.6",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prop-types": "^15.8.1"
  }
}
