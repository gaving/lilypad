{
  "name": "lilypad",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build && cp -r build ../server",
    "eject": "react-scripts eject",
    "lint": "DEBUG=eslint:cli-engine eslint --ignore-pattern 'build/*' --ignore-pattern 'public/*' --ext js --ext json --color .",
    "start": "concurrently 'react-scripts start' 'npm start --prefix ../server'",
    "test": "react-scripts test"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@blueprintjs/core": "^4.20.1",
    "@blueprintjs/select": "^4.9.22",
    "framer-motion": "^10.12.16",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-emoji-render": "^2.0.1",
    "react-redux": "^8.0.7",
    "react-router-dom": "^6.12.1",
    "react-scripts": "^5.0.1",
    "rebass": "^4.0.7",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "concurrently": "^8.2.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-json-files": "^2.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "http-proxy-middleware": "^2.0.6",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1"
  }
}
