    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-emoji-render": "^2.0.1",
    "react-router-dom": "^6.16.0",
    "rebass": "^4.0.7",
    "styled-components": "^6.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@vitejs/plugin-react": "^5.1.4",
    "concurrently": "^9.2.1",
    "prop-types": "^15.8.1",
    "vite": "^7.3.1"
