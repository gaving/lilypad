{
  "name": "lilypad",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build && cp -r build ../server",
    "eject": "react-scripts eject",
    "lint": "DEBUG=eslint:cli-engine eslint --ignore-pattern 'build/*' --ignore-pattern 'public/*' --ext js --ext json --color .",
    "start": "concurrently 'react-scripts start' 'npm start --prefix ../server'",
    "test": "react-scripts test"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@blueprintjs/core": "^4.5.1",
    "@blueprintjs/select": "^4.4.1",
    "framer-motion": "^6.3.16",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-emoji-render": "^1.2.4",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "rebass": "^4.0.7",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@babel/preset-react": "^7.18.6",
    "concurrently": "^7.2.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json-files": "^1.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "lint-staged": "^12.1.3",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "prop-types": "^15.6.2"
  }
}
